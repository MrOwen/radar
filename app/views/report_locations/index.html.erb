<h1>Listing report_locations</h1>

<table>
  <tr>
    <th>Detail</th>
    <th>Room</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @report_locations.each do |report_location| %>
  <tr>
    <td><%= report_location.detail %></td>
    <td><%= report_location.room %></td>
    <td><%= link_to 'Show', report_location %></td>
    <td><%= link_to 'Edit', edit_report_location_path(report_location) %></td>
    <td><%= link_to 'Destroy', report_location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

