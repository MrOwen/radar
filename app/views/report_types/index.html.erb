<h1>Listing Report Types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @report_types.each do |report_type| %>
  <tr>
    <td><%= report_type.name %></td>
    <td><%= link_to 'Show', report_type %></td>
    <td><%= link_to 'Edit', edit_report_type_path(report_type) %></td>
    <td><%= link_to 'Destroy', report_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

