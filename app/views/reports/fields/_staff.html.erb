<% if(type == "edit") %>
<% elsif(type == "search") %>
<% elsif(type == "index") %>
    <% if(part == "header") %>
        <%= link_to "Staff", params.merge(:sort => "submitter"), :remote => true %>
    <% elsif(part == "data") %>
        <%= report.staff.last_name rescue unknown %>
    <% end %>
<% elsif(type == "show") %>
  <div class="field">
    <%= label_tag  do -%>
	    <%= t "#{current_staff.organization}.submitter" %>:
	<% end -%>
    <%= @report.staff.name %>
  </div>   
<% end %>