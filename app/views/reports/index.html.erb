<div id="inside_container" class="inside_container">
  <div class="form_header">
  <% if @report_type != nil %>
      <%= "#{@report_type.display_name.pluralize}" %>
    <% else %>
      Reports
    <% end %>
	</div>
  <%= render :partial => "reports/index_#{@report_type.name.downcase}_generic", :locals => { :reports => @reports } %>
  <div class="new_report_link">
    <%= link_to 'New Report', new_report_path if can? :create, Report %>
    <div class="edit_link">
    </div>
  </div>
</div>
