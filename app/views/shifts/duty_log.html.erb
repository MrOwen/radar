<div id="inside_container" class="inside_container">
<h1>RA: <%= "#{@shift.staff.first_name} #{@shift.staff.last_name}" %></h1>
<h2>On Duty: <%= "#{@shift.start_date}, #{@shift.start_time} - #{@shift.end_date}, #{@shift.end_time}" %></h2>
<h2>Area: <%= "#{@shift.area.name}" %></h2>
</div>

<div>
	<% round_count = 0 %>
     <%  @rounds.each  do |round|  %>    
     	<% round_count = round_count + 1 %>
       <%= render :partial=> "shifts/round_in_duty_log", :locals => 
       { :round_count => round_count, :round => round, :reports =>report_map[round], :notes => note_map[round] } %>
     <% end %>
 </div>
