<h1>Listing staff_organizations</h1>

<table>
  <tr>
    <th>Staff</th>
    <th>Organization</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @staff_organizations.each do |staff_organization| %>
  <tr>
    <td><%= staff_organization.staff_id %></td>
    <td><%= staff_organization.organization_id %></td>
    <td><%= link_to 'Show', staff_organization %></td>
    <td><%= link_to 'Edit', edit_staff_organization_path(staff_organization) %></td>
    <td><%= link_to 'Destroy', staff_organization, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Staff organization', new_staff_organization_path if can? :create, StaffOrganization %>
